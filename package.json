{
  "name": "sofe",
  "version": "0.2.0",
  "description": "Service Oriented Front-end",
  "main": "dist/sofe.js",
  "scripts": {
    "test": "karma start",
    "build": "babel src --out-dir lib && node build.js",
    "rollup": "rollup src/index.js -f cjs -o dist/sofe.js",
    "clean": "rm -r lib",
    "prepublish": "npm run build",
